{
    "compilerOptions": {
      "target": "es2020",
      "strict": true,
      "preserveConstEnums": true,
      "sourceMap": true,
      "module": "CommonJS",
      "moduleResolution":"node",
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
  "include": ["src/**/*"],
  "exclude": ["**/*.test.ts"]
  }